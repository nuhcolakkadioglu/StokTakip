@model StokTakip.Entities.Dtos.KategoriDtos.KategoriListDto
<h2>kategori Anasayfa</h2>



<table class="table table-bordered">
	<thead>
		<tr>
			<th>#</th>
			<th>Adı</th>
			<th>Açıklama</th>
			<th>İşlemler</th>
		</tr>
	</thead>
	@foreach (var item in Model.Kategoriler)
	{
		<tr>
		<td>@item.Id</td>
		<td>@item.Adi</td>
		<td>@item.Aciklama</td>
		<td>
			<a class="btn btn-warning btn-sm">Düzenle</a>
			<a class="btn btn-danger btn-sm">Sil</a>
		</td>
	</tr>
	}
	<tbody>

	</tbody>
</table>